
@using Newtonsoft.Json;
<script type="text/javascript">
    document.title="Mapping";
    var content = @Html.Raw(JsonConvert.SerializeObject(TempData["MappingData"]));
    InitializeSelect();
</script>
<script src="~/Module_Script/master/mapping.js"></script>
<div class="esubmenu">
    <ul class="breadcrumb float-left">
        <li><a href="#">Home /</a></li>
        <li><a href="#">Pictures /</a></li>
        <li><a href="#">Summer 15 /</a></li>
        <li><a href="#" class="active">Summer 15</a></li>
    </ul>
    <div class="ever float-right">
        <span>Version : 0.0.1</span>
    </div>
</div>
<div class="pagebody">
    <div class="einrformbase card pt-2 pl-2 pr-2">
        <div class="card-title">
            Course Subject Mapping
        </div>
        <div class="card-body">
            <form name='MappingForm' id="MappingForm" method="post" noValidate onSubmit="return handleSubmit(this);">
                <ul class="einrform">
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="course">Course</label>
                                <select class="chosen-select" name="course" isRequired="true" id="ddlCourse">
                                    <option value="0">Select Course</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="semester">Semester</label>
                                <select class="chosen-select" name="semester" isRequired="true" id="ddlSemester">
                                    <option value="0">Select Semester</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="subject">Subject</label>
                                <select class="chosen-select" name="subject" isRequired="true" id="ddlSubject">
                                    <option value="0">Select Subject</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="active">Status</label>
                                <select class="chosen-select" name="active" isRequired="true" id="ddlActive">
                                    <option value="0">Select Status</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <input type="text" style="display:none" value="" name="mapId" />
                        <input type="text" style="display:none" value="A" name="flag" />
                        <input type="text" style="display:none" value="7" name="reportId" />
                        <button type="submit" class="btn btn-info"><span class="inload hide"><i class="fa fa-spinner fa-spin"></i></span> Save</button>
                    </li>
                </ul>
            </form>
            <div class="actionbse">
                <div id="mappingGrid" style="height:350px;" class="ag-theme-balham"></div>
            </div>
        </div>
    </div>
</div>
