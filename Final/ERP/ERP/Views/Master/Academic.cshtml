@using Newtonsoft.Json;
@{
    ViewBag.Title = "Academic";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<script type="text/javascript">
    var content = @Html.Raw(JsonConvert.SerializeObject(TempData["AcademicData"]));
</script>
<div class="esubmenu">
    <ul class="breadcrumb float-left">
        <li><a href="#">Home /</a></li>
        <li><a href="#">Pictures /</a></li>
        <li><a href="#">Summer 15 /</a></li>
        <li><a href="#" class="active">Summer 15</a></li>
    </ul>
    <div class="ever float-right">
        <span>Version : 0.0.1</span>
    </div>
</div>
<div class="pagebody">
    <div class="einrformbase card pt-2 pl-2 pr-2">
        <div class="card-title">
            Academic Year
        </div>
        <div class="card-body">
            <form name='AcademicYear' id="AcademicYear" noValidate onSubmit={this.handleSubmit}>
                <ul class="einrform">
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Academic Year</label>
                                <input type="text" class="form-control" name="academicYear" />
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="date">
                            <div class="form-group">
                                <label for="date">Start Date</label>
                                <input type="text" class="form-control" name="date" />
                            </div>
                            <span class="calendarbase"><img src="../../Images/calendar.png" /></span>
                        </div>
                    </li>
                    <li>
                        <div class="date">
                            <div class="form-group">
                                <label for="endDate">End Date</label>
                                <input type="text" class="form-control" name="endDate" />
                            </div>
                            <span class="calendarbase"><img src="../../Images/calendar.png" /></span>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="active">Status</label>
                                <select class="registration-form-control dropdown"></select>
                            </div>
                        </div>
                    </li>
                    <li>
                        <button type="submit" class="btn btn-info"><span class="inload hide"><i class="fa fa-spinner fa-spin"></i></span>Save</button>
                    </li>
                </ul>
            </form>
            <div class="actionbse">
                <div id="academicgrid" style="height:350px;" class="ag-theme-balham"></div>
            </div>
        </div>
    </div>
</div>

