@using Newtonsoft.Json;
<script type="text/javascript">
    document.title="Section";
    var content = @Html.Raw(JsonConvert.SerializeObject(TempData["SectionData"]));
    InitializeSelect();
</script>
<script src="~/Module_Script/master/section.js"></script>
<div class="esubmenu">
    <ul class="breadcrumb float-left">
        <li><a href="#">Home /</a></li>
        <li><a href="#">Pictures /</a></li>
        <li><a href="#">Summer 15 /</a></li>
        <li><a href="#" class="active">Summer 15</a></li>
    </ul>
    <div class="ever float-right">
        <span>Version : 0.0.1</span>
    </div>
</div>
<div class="pagebody">
    <div class="einrformbase card pt-2 pl-2 pr-2">
        <div class="card-title">
            Section
        </div>
        <div class="card-body">
            <form name='SectionForm' id="SectionForm" method="post" noValidate onSubmit="return handleSubmit(this);">
                <ul class="einrform">
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="course">Course</label>
                                <select class="chosen-select" name="courseId" id="ddlCourse">
                                    <option value="0">Select Course</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="semester">Semester</label>
                                <select class="chosen-select" name="semesterId" id="ddlSemester">
                                    <option value="0">Select Semester</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="sectionName">Section Name</label>
                                <select class="chosen-select" name="sectionName" id="ddlSection">
                                    <option value="0">Select Semester</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="active">Status</label>
                                <select class="chosen-select" name="isActive" id="ddlActive">
                                    <option value="0">Select Status</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <input type="text" style="display:none" value="" name="sectionId" />
                        <input type="text" style="display:none" value="A" name="flag" />
                        <input type="text" style="display:none" value="4" name="reportId" />
                        <button type="submit" class="btn btn-info"><span class="inload hide"><i class="fa fa-spinner fa-spin"></i></span>Save</button>
                    </li>
                </ul>
            </form>
            <div class="actionbse">
                <div id="sectionGrid" style="height:350px;" class="ag-theme-balham"></div>
            </div>
        </div>
    </div>
</div>