    document.title ="Time Table Configuration";
    @*var content = @Html.Raw(JsonConvert.SerializeObject(TempData["HolidayList"]));*@
    InitializeSelect();
    InitializeDate();

</script>
<script src="~/Module_Script/master/timetable.js"></script>
<div class="esubmenu">
    <ul class="breadcrumb float-left">
        <li><a href="#">Home /</a></li>
        <li><a href="#">Pictures /</a></li>
        <li><a href="#">Summer 15 /</a></li>
        <li><a href="#" class="active">Summer 15</a></li>
    </ul>
    <div class="ever float-right">
        <span>Version : 0.0.1</span>
    </div>
</div>
<div class="pagebody">
    <div class="einrformbase card pt-2 pl-2 pr-2">
        <div class="card-title">
            Time Table Configuration
        </div>
        <div class="card-body">
            <form name='TimeTable' id="TimeTable" method="post" noValidate onSubmit="return handleSubmit(this);">
                <ul class="einrform">
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Please Select</label>
                                <select class="chosen-select" id="ddlFor" isRequired="true" name="for">
                                    <option value="0" selected="selected">Select For</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Academic Year</label>
                                <select class="chosen-select" id="ddlAcademic" isRequired="true" name="acyr">
                                    <option value="0">Select Academic Year</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Course</label>
                                <select class="chosen-select" id="ddlCourse" isRequired="true" name="crs">
                                    <option value="0" selected="selected">Select Course</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Faculty</label>
                                <select class="chosen-select" id="ddlFac" isRequired="true" name="fac">
                                    <option value="0" selected="selected">Select Faculty</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Subject</label>
                                <select class="chosen-select" id="ddlFac" isRequired="true" name="fac">
                                    <option value="0" selected="selected">Select Subject</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Semester</label>
                                <select class="chosen-select" id="ddlSem" isRequired="true" name="sem">
                                    <option value="0" selected="selected">Select Semester</option>

                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Section</label>
                                <select class="chosen-select" id="ddlSec" isRequired="true" name="sec">
                                    <option value="0" selected="selected">Select Section</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="academicYear">Time Slot</label>
                                <select class="chosen-select" id="ddlSlot" isRequired="true" name="slot">
                                    <option value="0" selected="selected">Select Time Slot</option>
                                    <option value="0">9:00</option>
                                    <option value="0">Select Option</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="starttime">Start Time</label>
                                <input type="time" class="form-control" isRequired="true" name="starttime"  />
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="text">
                            <div class="form-group">
                                <label for="endtime">End Time</label>
                                <input type="time" class="form-control" isRequired="true" name="endtime" />
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <div class="ddl">
                            <div class="form-group">
                                <label for="active">Status</label>
                                <select class="chosen-select" id="ddlActive" isRequired="true" name="active">
                                    <option value="0">Select Status</option>
                                </select>
                                <span class="field-validation-error"></span>
                            </div>
                        </div>
                    </li>
                    <li>
                        <input type="text" style="display:none" value="" name="yearId" />
                        <input type="text" style="display:none" value="A" name="flag" />
                        <input type="text" style="display:none" value="1" name="reportId" />

                        <button type="submit" class="btn btn-info"><span class="inload hide"><i class="fa fa-spinner fa-spin"></i></span>Save</button>
                    </li>
                </ul>
            </form>
            <div class="actionbse">
                <div id="timetableGrid" style="height:350px;" class="ag-theme-balham"></div>
            </div>
        </div>
    </div>
</div>

